#include "Character.h"
#include "Player.h"
#include <math.h>
#include <iostream>
void Player::moveLeft() {
  posX -= speed;
}

void Player::moveRight() {
  posX += speed;
}

void Player::jump() {
  ySpeed = 5;
}

void Player::update(Matrix& mat) {
  if(mat.at(floor(posX/32), floor(posY/32)+1) == 0) {
    posY++;
    std::cout << mat.at(floor(posX/32), floor(posY/32)+32) << std::endl;
  }
  if(ySpeed > 0)
    ySpeed--;
  posY += ySpeed;
}
